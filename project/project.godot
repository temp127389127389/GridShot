; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GridShot"
config/tags=PackedStringArray("school")
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
run/max_fps=60
config/icon="res://icon.svg"
boot_splash/minimum_display_time=2000

[autoload]

Config="*res://global scripts/config.gd"
Network="*res://global scripts/network.gd"
Globals="*res://global scripts/globals.gd"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="viewport"
window/stretch/scale=0.6
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/dev_time_tracker/plugin.cfg", "res://addons/webrtc-multiplayer/plugin.cfg")

[file_customization]

folder_colors={
"res://global scripts/": "purple",
"res://scenes/": "green",
"res://scenes/PlayerCameraEffects/": "blue",
"res://scenes/Projectiles/": "red",
"res://scenes/UI/": "yellow",
"res://script classes/": "pink"
}

[filesystem]

import/blender/enabled=false

[global_group]

players=""
"screen flash"=""
"player stun"=""
"team menu player bar"=""

